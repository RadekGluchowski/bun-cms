{
  "name": "bun-cms-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "setup": "bun run scripts/dev-setup.ts",
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "db:generate": "bun run --cwd apps/backend db:generate",
    "db:migrate": "bun run --cwd apps/backend db:migrate",
    "db:seed": "bun run --cwd apps/backend db:seed"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "packageManager": "bun@1.1.42",
  "engines": {
    "node": ">=20.0.0"
  }
}
